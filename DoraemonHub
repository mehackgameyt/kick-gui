-- Khi b·∫£ng hi·ªán, n√∫t m·ªü ·∫©n, khi b·∫£ng ·∫©n th√¨ n√∫t m·ªü hi·ªán
openButton.MouseButton1Click:Connect(function()
    mainFrame.Visible = true
    openButton.Visible = false
end)

-- T·ª± ƒë·ªông t√¨m t√™n tr√πng theo 1 k√Ω t·ª± ho·∫∑c nhi·ªÅu k√Ω t·ª±, cho ph√©p s·ªë v√† k√Ω t·ª±
nameBox:GetPropertyChangedSignal("Text"):Connect(function()
    local txt = nameBox.Text
    if #txt > 0 then
        for _,plr in pairs(Players:GetPlayers()) do
            if plr.Name:lower():sub(1,#txt) == txt:lower() then
                if nameBox.Text ~= plr.Name then
                    nameBox.Text = plr.Name
                    nameBox.CursorPosition = #plr.Name + 1
                end
                break
            end
        end
    end
end)

-- H√†m copy to√†n b·ªô ƒë·ªì trong Backpack ng∆∞·ªùi ƒë∆∞·ª£c ch·ªçn sang balo m√¨nh
local function copyBackpack(targetName)
    local targetPlayer = Players:FindFirstChild(targetName)
    if not targetPlayer then return end
    if not targetPlayer:FindFirstChild("Backpack") then return end
    local targetBackpack = targetPlayer.Backpack
    local myBackpack = player.Backpack
    for _, item in pairs(targetBackpack:GetChildren()) do
        -- Clone m√≥n ƒë·ªì r·ªìi cho v√†o balo m√¨nh
        if item:IsA("Tool") then
            local cloneItem = item:Clone()
            cloneItem.Parent = myBackpack
        end
    end
end

-- N√∫t tr·ªôm ƒë·ªì
stealButton.MouseButton1Click:Connect(function()
    local targetName = nameBox.Text
    if targetName == "" then
        stealButton.BackgroundColor3 = Color3.fromRGB(150,150,150) -- x√°m n·∫øu r·ªóng t√™n
        return
    end
    stealButton.BackgroundColor3 = Color3.fromRGB(255,0,0)
    copyBackpack(targetName)
end)

